<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd"
      start-state="bodega_taller">
    
    <input name="isChildFlow" type="java.lang.Boolean"/>
    
    <on-start>
        <evaluate expression="mantenedorBodegaTaller.init()"/>
    </on-start>
    
    <view-state id="bodega_taller">
        <transition on="stock" to="ingresoProducto"/>
    </view-state>
    
    <view-state id="ingresoProducto">
        <transition on="back" to="end_flow"/>
        <transition on="output" to="salidaProducto"/>
    </view-state>
    
    <view-state id="salidaProduto"></view-state>
    
    <end-state id="end_flow"/>
</flow>