<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" start-state="listar"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

    <on-start>
        <evaluate expression="mantenedorEquipos.inicio()"/>
    </on-start>

    <view-state id="listar">
        <transition on="flowEditar" to="editarEquipo"/>
    </view-state>

    <view-state id="editarEquipo">

    </view-state>
</flow>